{
  "name": "vue-slide-demo-spa",
  "version": "1.0.0",
  "description": "Vue 3 SPA with multi-step slide form system",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit -p tsconfig.app.json && vite build",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest",
    "test:e2e": "playwright test"
  },
  "keywords": [
    "vue",
    "vue3",
    "typescript",
    "spa",
    "slide-form",
    "tailwindcss",
    "pinia"
  ],
  "author": "Brenden Chu",
  "license": "MIT",
  "dependencies": {
    "@tsparticles/slim": "^3.9.1",
    "@tsparticles/vue3": "^3.0.1",
    "@vueuse/core": "^11.3.0",
    "@vueuse/head": "^2.0.0",
    "axios": "^1.13.2",
    "localforage": "^1.10.0",
    "pinia": "^2.3.1",
    "vue": "^3.5.26",
    "vue-router": "^4.6.4",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@tailwindcss/forms": "^0.5.11",
    "@tailwindcss/typography": "^0.5.19",
    "@types/node": "^22.19.5",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitest/coverage-v8": "^4.0.16",
    "@vitest/ui": "^4.0.16",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.23",
    "daisyui": "^4.12.24",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-vue": "^10.6.2",
    "happy-dom": "^20.1.0",
    "postcss": "^8.5.6",
    "prettier": "^3.7.4",
    "tailwindcss": "^3.4.19",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.52.0",
    "vite": "^6.4.1",
    "vitest": "^4.0.16",
    "vitest-localstorage-mock": "^0.1.2",
    "vue-tsc": "^2.2.12"
  }
